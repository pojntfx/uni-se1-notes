   "source": [
    "# `int` to `short` assignment using `final`"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "class Test {\n",
    "    static short test() {\n",
    "        final short a = 4;\n",
    "        \n",
    "        return a + 7;\n",
    "    }\n",
    "}"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [
    {
     "data": {
